{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "allowJs": false,
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./",
    "paths": {
      "@src/*": ["src/*"],
      "@whitelabel/*": ["whitelabel/*"]
    }
  },
  "include": ["**/*.ts", "**/*.tsx", "svg.d.ts"],
  "typeRoots": ["node_modules/@types"]
}
